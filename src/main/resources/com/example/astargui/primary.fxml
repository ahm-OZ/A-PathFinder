<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@primary.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.astargui.Controller">
    <HBox prefHeight="24.0" prefWidth="800.0" stylesheets="@primary.css">
        <Button onAction="#setStart" text="Set Start" />
        <Button onAction="#setEnd" text="Set End" />
        <Button onAction="#runAlgorithm" text="Run" />
        <Button onAction="#drawWalls" text="Draw Walls" />
      <Button mnemonicParsing="false" onAction="#deleteWalls" text="Erase Walls" />
        <Button onAction="#clearGrid" prefHeight="26.0" prefWidth="93.0" text="Clear" />
    </HBox>
    <Canvas fx:id="canvas" height="500" width="800.0" />
</VBox>
